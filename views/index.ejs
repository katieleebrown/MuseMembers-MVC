<body class="d-flex flex-column min-vh-100">
    <%- include('partials/header') -%>

        <!-- Showcase -->
        <section class="p-5 p-lg-5 pt-lg-5 text-center text-sm-start">
            <div class="container">
                <div class="d-sm-flex align-items-center justify-content-between">
                    <div>
                        <h1 class="display-3">Your museum memberships, at your fingertips</h1>
                        <p class="lead my-4">Looking for a quick and easy way to keep all of your Museum memberships in
                            one
                            place? Feel like your wallet is overflowing? MuseMembers is here to help. We're an
                            accessible,
                            easy to use web app that houses all of your membership information in one place to help you
                            access anywhere, anytime.

                        </p>
                        <p class="lead my-4">If your museum or organization is a current partner, you're in luck!
                            MuseMembers works with your cultural institution to give you access to your
                            membership - and it's benefits - no matter where you are. Are you looking to get more out of
                            your membership? Tell your team about MuseMembers today!</p>
                        <button class="btn btn-primary btn-lg shadow voltaire" data-bs-toggle="modal"
                            data-bs-target="#enroll">Register</button>
                    </div>
                    <img class="img-fluid w-50 d-none d-sm-block" src="/images/womanAndArt.svg" alt="">
                </div>
            </div>
        </section>

        <!-- Newsletter -->
        <section class="bg-dark p-5 text-primary">
            <div class="container">
                <div class="d-md-flex justify-content-between align-items-center">
                    <h3 class="mb-3 mb-md-0">Sign Up For Our Newsletter</h3>

                    <div class="input-group news-input">
                        <input type="text" class="form-control p-4" placeholder="Enter Email">
                        <button class="btn btn-primary btn-lg voltaire" type="button">Submit</button>
                    </div>
                </div>
            </div>
        </section>

        <!-- Question Accordion -->
        <section id="questions" class="p-5">


        </section>

        <!-- Partners -->
        <section class="bg-light p-5">
            <div class="container">
                <h3 class="mb-5 mb-md-0 text-center pb-3">Our Partners</h3>
                <div class="d-md-flex justify-content-between align-items-center">
                    <div class="row g4">
                        <div class="col-sm-6 col-md-3 my-2">
                            <div class="card bg-white">
                                <div class="card-body text-center">
                                    <img src="/images/Cary Children's Museum.png" class="mb-3 img-fluid" alt="">
                                    <h5 class="card-title mb-3">Cary Children's Museum</h5>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-md-3 my-2">
                            <div class="card bg-white">
                                <div class="card-body text-center">
                                    <img src="/images/City of Oaks Historical Center.png" class="mb-3 img-fluid" alt="">
                                    <h5 class="card-title mb-3">City of Oaks Historical Center</h5>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-md-3 my-2">
                            <div class="card bg-white">
                                <div class="card-body text-center">
                                    <img src="/images/Museum of Climate Sciences.png" class="mb-3 img-fluid" alt="">
                                    <h5 class="card-title mb-3">Museum of Climate Sciences</h5>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-md-3 my-2">
                            <div class="card bg-white">
                                <div class="card-body text-center">
                                    <img src="/images/North Carolina Farm Park.png" class="mb-3 img-fluid" alt="">
                                    <h5 class="card-title mb-3">North Carolina Farm Park</h5>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <%- include('partials/footer') -%>

            <!-- For Bootstrap -->
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
                integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
                crossorigin="anonymous"></script>

</body>

</html>